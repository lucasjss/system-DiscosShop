<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: #31081f;"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.sonodisco.GraficoController">
    <children>
        <VBox prefHeight="500.0" prefWidth="500.0">
            <children>
                <PieChart fx:id="grafico" prefHeight="410.0" prefWidth="500.0"/>
                <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#voltar" prefHeight="50.0" prefWidth="100.0"
                        style="-fx-background-color: white;" text="Voltar">
                    <VBox.margin>
                        <Insets left="200.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="Comic Sans MS" size="20.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
